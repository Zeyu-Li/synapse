{
  "name": "synapse",
  "version": "0.0.1",
  "description": "Synapse is a tool that generates interactive graphs of course prerequisites and corequisites using data extracted from the UAlberta course catalogue.",
  "main": "index.js",
  "dependencies": {
    "copyfiles": "^2.4.1"
  },
  "devDependencies": {
    "@types/puppeteer": "^5.4.3",
    "nodemon": "^2.0.7",
    "puppeteer": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/**/*.html src/**/*.css dist/",
    "scrape": "tsc ./scraper/scraper.ts ./scraper/pool.ts && node scraper/scraper.js",
    "start": "npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --watch src --exec \"npm run build\" -e html,css,ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steventango/synapse.git"
  },
  "author": "Steven Tang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steventango/synapse/issues"
  },
  "homepage": "https://github.com/steventango/synapse#readme"
}
